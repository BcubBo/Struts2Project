<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Conifguration 2.3//EN" 
	"D:\JARPack\struts2lib\struts-2.3.dtd">

<struts>
 <package name="house" namespace="/house" extends="default">
		<!-- 进入添加页面 -->
		
		<action name="addHouse" class="action.HouseAction">
			
			<result>/add_house.jsp</result>
		
		
		</action>
		
		
		<!-- 提交房屋页面 -->
		<action name="doAddHouse" class="action.HouseAction" method="doAddHouse" >
			
			<result>/manage.jsp</result>
		
		
		</action>
		<!-- 进入搜索页面 -->
		<action name="search" class="action.HouseAction" method="search">
		
			<result>/manage.jsp</result>
		
		</action>
		<action name="findAll" class="action.HouseAction" method="findAll"></action>
		
		
		
		
		<!-- 执行搜索页面 -->
</package>
</struts>
